Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                // 
                echo 'Etape compile'
            }
        }
        
        stage('Test') { 
            steps {
                // 
                echo 'Etape Test'
            }
        }
        
        stage('Deploy') { 
            steps {
                // 
                echo 'Etape Packaging'
            }
            
         stage('Test') { 
            steps {
                // 
                echo 'Etape Deploy'
            }
        }
    }
}
